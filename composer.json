{
	"name": "vasek-purchart/doctrine-date-time-immutable-types-bundle",
	"description": "Bundle integration of Doctrine DateTimeImmutable types for Symfony",
	"keywords": ["symfony", "bundle", "doctrine", "type", "datetime", "immutable", "datetimeimmutable", "dbal"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~5.6|~7.0",
		"doctrine/doctrine-bundle": "~1.3",
		"symfony/config": "~2.6",
		"symfony/dependency-injection": "~2.6",
		"symfony/http-kernel": "~2.6",
		"symfony/yaml": "~2.6",
		"vasek-purchart/doctrine-date-time-immutable-types": "~0.2.0"
	},
	"require-dev": {
		"consistence/coding-standard": "0.8",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9",
		"phing/phing": "2.11.0",
		"phpunit/phpunit": "4.7.5"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\DoctrineDateTimeImmutableTypesBundle\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\DoctrineDateTimeImmutableTypesBundle\\": "tests"
		}
	},
	"config": {
